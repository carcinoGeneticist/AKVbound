    <rooms>

        <room name='lowaa' walkableMap='lowaa_WM' scale='4'>


            <triggers>

                <trigger>spriteProperty,char,x&lt;780
                    <trigger>spriteProperty,char,y&gt;1090
                        <trigger>
                            <args>gameState,leaveLOWAA=false</args>
                            <action command='goBack'>
                                <args>char</args>
                                <action command='talk'>
                                    <args>
                                        @! You're not quite ready to leave the bubble yet.
                                    </args>
                                </action>
                            </action>
                        </trigger>
                    </trigger>
                </trigger>

                <trigger>spriteProperty,char,x&lt;735
                    <trigger>spriteProperty,char,y&gt;1090
                        <trigger>
                            <args>gameState,leaveLOWAA=true</args>                        
                                <action command='changeRoomRemote'>
                                    <args>rooms/lab1.xml,lab1,1690,3035</args>
                                </action>
                        </trigger>
                    </trigger>
                </trigger>


                <trigger detonate='true'>spriteProperty,char,x&gt;1
                    <action command='setGameState'>
                        <args>leaveLOWAA,false</args>
                    </action>
                </trigger>

                <!-- tavros conversation -->
                <trigger><args>gameState,showTav=true</args>
                    <trigger>spriteProperty,eridan,y&gt;970
                        <trigger detonate = 'true'> spriteProperty,eridan,x&gt;1510
                            <action class='tavrosTalk1' command='talk' noDelay='true'>
                                <action command='unfollow'>
                                    <args>eridan</args>
                                    <action command='setGameState'>
                                        <args>leaveLOWAA,true</args>
                                        <action command='changeRoomRemote'>
                                            <args>rooms/lab1.xml,lab1,1690,3035</args>
                                            <action command='walk'>
                                                <args>char,Left</args>
                                            </action>
                                        </action>
                                    </action>
                                </action>
                            </action>
                        </trigger>
                    </trigger>
                </trigger>


                <!-- eridan conversation at top of lowaa 2-->
                <trigger>spriteProperty,eridan,x&lt;1100
                	<trigger detonate='true'>spriteProperty,eridan,y&lt;450
                		<action command='removeSprite'>
                			<args>davelowaa,lowaa</args>
                		</action>
                	</trigger>
                </trigger>

                <!-- eridan conversation at top of lowaa -->
                <trigger>spriteProperty,eridan,x&lt;1100
                    <trigger detonate='true'>spriteProperty,eridan,y&lt;400
                        <action class='eridanTalk3' command='talk' noDelay='true'>
                            <action command='moveSprite'> 
                                <args>tavros,1680,1020</args>
                                <action command='setGameState'>
                                    <args>showTav,true</args>
                                </action>
                            </action>
                        </action>
                    </trigger>
                </trigger>
              
            </triggers>


            <!-- eridan 980 x 790 -->
            <character class='eridan' x='2000' y='50'>
                <action name='Talk to Eridan.' command='talk'>
                    <args>
                        @eridan_huh ERIDAN: so kar are you all alivve
                    </args>
                    <action command='follow'>
                        <args>eridan,char</args>
                        <action command='removeAction'>
                            <args>eridan,Talk to Eridan.</args>
                        </action>
                    </action>
                </action>

            </character>

            <character class='terezilowaa' x='869' y='771'>
                <action command='talk' name='Talk to Terezi.'>
                    <args>
                    @karkat_talk KARKAT: I GUESS YOUR ROLEPLAY PLANS CAN’T REALLY GO THROUGH NOW, HUH?
                    </args>
                </action>
            </character>

            <character class='davelowaa' x='918' y='841'>
                <action command='talk' name='Talk to Dave.'>

                    <args>
                        @davelowaa_smug DAVE: so who the fuck’s memories are these? im kind of digging the set
                    </args>

                    <action command='removeAction' >
                        <args>davelowaa,Talk to Dave.</args>
                        <action command='moveSprite'> 
                            <args>eridan,980,790</args>
                        </action>
                    </action>

                </action>
            </character>


            <!-- tavros 1680 x 1020 -->
            <character class='tavros' x='2000' y='50'>
                <action command='talk' name='Talk to Tavros.'>
                    <args>
                        @tavros_blink TAVROS: hEY,,,
                    	@karkat_horror KARKAT: FUCK!
                    </args>
                    <action command='unfollow'>
                                <args>eridan</args>
                    <action command='setGameState'>
                        <args>leaveLOWAA,true</args>
                    </action>
                </action>
                </action>
            </character>

            <character class='karkat' x='980' y='790'>
            </character>
        



